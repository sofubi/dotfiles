# vim: set ft=zsh:

## Init
source $ZDOTDIR/rc.d/init

## Path
source $ZDOTDIR/rc.d/path

## Env
source $ZDOTDIR/rc.d/env

## Hist
source $ZDOTDIR/rc.d/history

## Colors
source $ZDOTDIR/rc.d/colors

## Completion
source $ZDOTDIR/rc.d/completion

## File System
source $ZDOTDIR/rc.d/fs

## Antidote
source $ZDOTDIR/rc.d/antidote

## Utils
source $ZDOTDIR/rc.d/utils

## Plugin config
source $ZDOTDIR/rc.d/plugins

## Aliases / Functions
source $ZDOTDIR/rc.d/aliases
source $ZDOTDIR/rc.d/functions

## Prompt
source $ZDOTDIR/rc.d/prompt
